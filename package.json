{
  "name": "mach-vscode",
  "displayName": "Mach",
  "description": "Language support for Mach - a statically-typed, compiled programming language designed to be simple, fast, verbose, and intuitive",
  "version": "4.0.0",
  "publisher": "octalide",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "icon": "mach_128.png",
  "main": "./extension.js",
  "contributes": {
    "languages": [
      {
        "id": "mach",
        "aliases": [
          "Mach",
          "mach"
        ],
        "extensions": [
          ".mach"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./mach_file_icon.svg",
          "dark": "./mach_file_icon.svg"
        }
      }
    ],
    "grammars": [
      {
        "language": "mach",
        "scopeName": "source.mach",
        "path": "./syntaxes/mach.tmLanguage.json"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/octalide/mach-vscode"
  }
}
